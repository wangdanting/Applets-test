<!--index.wxml-->
<view class="map_container">
   <map id="map"
        class="map"
        longitude="{{longitude}}"
        latitude="{{latitude}}"
        scale="14"
        markers="{{markers}}"></map>
</view>
<view class="map_text">
    <text class="h1">{{textData.name}}</text>
    <text>{{textData.desc}}</text>
</view>
